{
  "name": "soap-api",
  "version": "1.0.0",
  "author": "Bogdan Petcu",
  "description": "A simple REST API that call other apis via soap",
  "main": "app.ts",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --verbose",
    "dev": "nodemon --ext ts --exec tsx src/app.ts | pino-pretty --colorize",
    "migrate": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "migrate:prod": "prisma migrate deploy",
    "db:generate": "prisma generate"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@fastify/autoload": "^6.3.0",
    "@fastify/awilix": "^7.0.0",
    "@fastify/env": "^5.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.6.0",
    "argon2": "^0.41.1",
    "awilix": "^12.0.5",
    "axios": "^1.8.4",
    "fast-xml-parser": "^5.2.1",
    "fastify": "^5.3.2",
    "fastify-plugin": "^5.0.0",
    "flydrive": "^1.1.1",
    "luxon": "^3.6.1",
    "ofetch": "^1.4.1",
    "pino": "^9.2.0",
    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.14.1",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.9.18",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "esbuild": "^0.25.2",
    "fastify-tsconfig": "^3.0.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "13.0.0",
    "prisma": "^6.6.0",
    "resolve-tspaths": "0.8.23",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3"
  }
